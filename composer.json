{
    "name": "cypresslab/angular-ng-routing-bundle",
    "type": "symfony-bundle",
    "description": "A way to expose your Symfony2 routing to Angular $resource service.",
    "keywords": [ "angular.js", "js routing", "routing", "javascript" ],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Matteo Giachino",
            "email": "matteog@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "friendsofsymfony/jsrouting-bundle": "~1.5"
    },
    "autoload": {
        "psr-0": { "Cypress\\AngularNgRoutingBundle": "" }
    },
    "target-dir": "Cypress/AngularNgRoutingBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
